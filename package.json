    "compass-mixins": "0.12.10",
    "css-loader": "5.0.2",
    "css-minimizer-webpack-plugin": "1.3.0",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "1.3.8",
    "node-sass": "4.14.1",
    "resolve-url-loader": "3.1.2",
    "rimraf": "3.0.2",
    "sass-loader": "11.0.1",
