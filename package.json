{
  "name": "cds-theme",
  "version": "0.0.1",
  "description": "Build for CDS Theme",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "setup": "npm ci",
    "build": "npm run clean && webpack --config ./style.config.js --color && npm run clean:js",
    "build-dev:": "npm run build",
    "build-prod:": "npm run build",
    "clean:js": "rimraf webapp/production/Connector/resources/main.js",
    "clean:css": "rimraf webapp/production/Connector/resources/Connector-all.css",
    "clean": "npm run clean:js && npm run clean:css"
  },
