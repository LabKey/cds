{
  "name": "cds-theme",
  "version": "0.0.1",
  "description": "Build for CDS Theme",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "setup": "npm ci",
    "build:connector": "npm run clean:connector && webpack --config ./themes/build/connector.config.js --color && npm run clean:connector-js",
    "build:front-page": "npm run clean:front-page && webpack --config ./themes/build/front-page.config.js --color && npm run clean:front-page-js",
    "build": "npm run build:connector && npm run build:front-page",
    "build-dev": "npm run build",
    "build-prod": "npm run build",
    "clean:connector-js": "rimraf webapp/production/Connector/resources/main.js",
    "clean:connector-css": "rimraf webapp/production/Connector/resources/Connector-all.css",
    "clean:connector": "npm run clean:connector-js && npm run clean:connector-css",
    "clean:front-page-js": "rimraf webapp/frontPage/css/main.js",
    "clean:front-page-css": "rimraf webapp/frontPage/css/application.css",
    "clean:front-page": "npm run clean:front-page-js && npm run clean:front-page-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/cds.git"
  },
  "author": "alanv@labkey.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LabKey/cds/issues"
  },
  "homepage": "https://github.com/LabKey/cds#readme",
  "dependencies": {},
  "devDependencies": {
    "bourbon": "4.2.7",
    "bourbon-neat": "1.9.1",
    "compass-mixins": "0.12.10",
    "css-loader": "5.0.2",
    "css-minimizer-webpack-plugin": "1.3.0",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "1.3.8",
    "node-sass": "4.14.1",
    "resolve-url-loader": "3.1.2",
    "rimraf": "3.0.2",
    "sass-loader": "11.0.1",
    "style-loader": "2.0.0",
    "webpack": "5.21.2",
    "webpack-cli": "4.5.0"
  }
}
