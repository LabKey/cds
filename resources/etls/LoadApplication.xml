<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Load CDS Application Tables</name>
    <description>Populate tables used to drive the Dataspace application</description>
    <transforms>
        <transform id="PopulateStudies" type="TaskrefTransformStep">
            <taskref ref="org.labkey.cds.data.steps.PopulateStudiesTask">
            </taskref>
        </transform>
        <transform id="studies" type="org.labkey.di.pipeline.TransformTask">
            <source schemaName="study" queryName="ds_study" containerFilter="CurrentAndSubfolders" />
            <destination schemaName="cds" queryName="study" targetOption="truncate" />
        </transform>
    </transforms>
</etl>
