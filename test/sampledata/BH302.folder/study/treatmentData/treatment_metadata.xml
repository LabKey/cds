<?xml version="1.0" encoding="UTF-8"?>
<tables xmlns="http://labkey.org/data/xml">
  <table tableName="ProductAntigen" tableDbType="TABLE">
    <description>Contains one row per study product antigen</description>
    <columns/>
  </table>
  <table tableName="Product" tableDbType="TABLE">
    <description>Contains one row per study product</description>
    <columns>
      <column columnName="Developer">
        <datatype>varchar</datatype>
        <columnTitle>Developer</columnTitle>
      </column>
      <column columnName="Immunogen">
        <datatype>varchar</datatype>
        <columnTitle>Immunogen</columnTitle>
      </column>
      <column columnName="Class">
        <datatype>varchar</datatype>
        <columnTitle>Class</columnTitle>
      </column>
      <column columnName="VectorClass">
        <datatype>varchar</datatype>
        <columnTitle>Vector Class</columnTitle>
        <description>ex. Canarypox, Adenovirus, etc.</description>
      </column>
      <column columnName="ProductSubclass">
        <datatype>varchar</datatype>
        <columnTitle>Product Subclass</columnTitle>
        <description>ex. HIV</description>
      </column>
      <column columnName="Description">
        <datatype>varchar</datatype>
        <inputType>textarea</inputType>
        <columnTitle>Description</columnTitle>
      </column>
      <column columnName="DeveloperContact">
        <datatype>varchar</datatype>
        <columnTitle>Developer Contact</columnTitle>
      </column>
      <column columnName="Manufacturer">
        <datatype>varchar</datatype>
        <columnTitle>Manufacturer</columnTitle>
      </column>
      <column columnName="Production">
        <datatype>varchar</datatype>
        <inputType>textarea</inputType>
        <columnTitle>Production</columnTitle>
      </column>
      <column columnName="ToxicityStudies">
        <datatype>varchar</datatype>
        <inputType>textarea</inputType>
        <columnTitle>Toxicity Studies</columnTitle>
      </column>
      <column columnName="PreviousTrials">
        <datatype>varchar</datatype>
        <inputType>textarea</inputType>
        <columnTitle>Previous Trials</columnTitle>
      </column>
      <column columnName="INDNumbers">
        <datatype>varchar</datatype>
        <columnTitle>IND #s</columnTitle>
      </column>
      <column columnName="INDHolders">
        <datatype>varchar</datatype>
        <columnTitle>IND Holders</columnTitle>
      </column>
    </columns>
  </table>
  <table tableName="TreatmentProductMap" tableDbType="TABLE">
    <description>Contains one row per study treatment product</description>
    <columns/>
  </table>
  <table tableName="Treatment" tableDbType="TABLE">
    <description>Contains one row per study treatment</description>
    <columns/>
  </table>
</tables>