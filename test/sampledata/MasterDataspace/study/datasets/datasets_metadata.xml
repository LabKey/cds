  <table tableName="ELISPOT" tableDbType="TABLE">
    <description>Contains up to one row of ELISPOT (Enzyme-Linked ImmunoSpot) data for each Subject/Visit combination.</description>
    <columns>
      <column columnName="SubjectId">
        <datatype>varchar</datatype>
        <columnTitle>Subject Id</columnTitle>
        <description>Subject identifier</description>
        <propertyURI>http://cpas.labkey.com/Study#ParticipantId</propertyURI>
        <nullable>false</nullable>
        <importAliases>
          <importAlias>ptid</importAlias>
        </importAliases>
        <fk>
          <fkDbSchema>study</fkDbSchema>
          <fkTable>Subject</fkTable>
          <fkColumnName>SubjectId</fkColumnName>
        </fk>
      </column>
      <column columnName="SequenceNum">
        <datatype>double</datatype>
        <columnTitle>Sequence Num</columnTitle>
        <propertyURI>http://cpas.labkey.com/Study#SequenceNum</propertyURI>
        <nullable>false</nullable>
        <importAliases>
          <importAlias>visit</importAlias>
          <importAlias>visitid</importAlias>
        </importAliases>
        <formatString>#</formatString>
      </column>
      <column columnName="date">
        <datatype>timestamp</datatype>
        <columnTitle>Date</columnTitle>
        <propertyURI>http://cpas.labkey.com/Study#VisitDate</propertyURI>
      </column>
      <column columnName="visit_day">
        <datatype>integer</datatype>
        <columnTitle>Visit Day</columnTitle>
        <rangeURI>http://www.w3.org/2001/XMLSchema#int</rangeURI>
      </column>
      <column columnName="third_key">
        <datatype>varchar</datatype>
        <columnTitle>Third Key</columnTitle>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <isKeyField>true</isKeyField>
      </column>
      <column columnName="assay_identifier">
        <datatype>varchar</datatype>
        <columnTitle>Assay Identifier</columnTitle>
        <description>Name identifying assay</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
      </column>
      <column columnName="antigen">
        <datatype>varchar</datatype>
        <columnTitle>Antigen Panel</columnTitle>
        <description>The name of the panel of antigens (peptide pools) for which a magnitude and response call are calculated. The magnitude and response calls are the maximum of the values of magnitude and response for thegene-based sub-panels making up the panel.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="cell_type">
        <datatype>varchar</datatype>
        <columnTitle>Cell Type</columnTitle>
        <description>Type of cell or cell subset assessed for antigen-stimulated expression of peptide(s). This cell type is usually CD4+ or CD8+ T-cells, but other cell types can also be assessed.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="specimen_type">
        <datatype>varchar</datatype>
        <columnTitle>Specimen type</columnTitle>
        <description>The type of specimen used in the assay.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="functional_marker_name">
        <datatype>varchar</datatype>
        <columnTitle>Functional Marker Name</columnTitle>
        <description>The name of the marker assessed, (IFNg, cytokine).</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="response_call">
        <datatype>boolean</datatype>
        <columnTitle>Response call</columnTitle>
        <description>Indicates if response was detected as calculated using bootstrap response call.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#boolean</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="magnitude">
        <datatype>integer</datatype>
        <columnTitle>Magnitude (% cells) - Background subtracted</columnTitle>
        <description>Mean Number of Spot-forming Cells (SFC) in response to antigen stimulation, adjusted for number of spot forming cells without antigen stimulation.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#double</rangeURI>
      </column>
      <column columnName="magnitude_raw">
        <datatype>double</datatype>
        <columnTitle>Magnitude (% cells) - Raw</columnTitle>
        <description>Mean Number of Spot-forming Cells (SFC) in response to antigen stimulation.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#double</rangeURI>
      </column>
      <column columnName="magnitude_background">
        <datatype>double</datatype>
        <columnTitle>Magnitude (% cells) - Background</columnTitle>
        <description>Mean Number of Spot-forming Cells (SFC) in the absence of antigen stimulation.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#double</rangeURI>
      </column>
    </columns>
    <tableTitle>Enzyme-Linked ImmunoSpot</tableTitle>
  </table>
  <table tableName="NAb" tableDbType="TABLE">
    <description>Contains up to one row of NAb (Neutralizing antibody) data for each Subject/Visit combination.</description>
    <columns>
      <column columnName="SubjectId">
        <datatype>varchar</datatype>
        <columnTitle>Subject Id</columnTitle>
        <description>Subject identifier</description>
        <propertyURI>http://cpas.labkey.com/Study#ParticipantId</propertyURI>
        <nullable>false</nullable>
        <importAliases>
          <importAlias>ptid</importAlias>
        </importAliases>
        <fk>
          <fkDbSchema>study</fkDbSchema>
          <fkTable>Subject</fkTable>
          <fkColumnName>SubjectId</fkColumnName>
        </fk>
      </column>
      <column columnName="SequenceNum">
        <datatype>double</datatype>
        <columnTitle>Sequence Num</columnTitle>
        <propertyURI>http://cpas.labkey.com/Study#SequenceNum</propertyURI>
        <nullable>false</nullable>
        <importAliases>
          <importAlias>visit</importAlias>
          <importAlias>visitid</importAlias>
        </importAliases>
        <formatString>#</formatString>
      </column>
      <column columnName="date">
        <datatype>timestamp</datatype>
        <columnTitle>Date</columnTitle>
        <propertyURI>http://cpas.labkey.com/Study#VisitDate</propertyURI>
      </column>
      <column columnName="third_key">
        <datatype>varchar</datatype>
        <columnTitle>Third Key</columnTitle>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <isKeyField>true</isKeyField>
      </column>
      <column columnName="visit_day">
        <datatype>integer</datatype>
        <columnTitle>Visit Day</columnTitle>
        <rangeURI>http://www.w3.org/2001/XMLSchema#int</rangeURI>
      </column>
      <column columnName="assay_identifier">
        <datatype>varchar</datatype>
        <columnTitle>Assay Identifier</columnTitle>
        <description>Name identifying assay</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="lab">
        <datatype>integer</datatype>
        <columnTitle>Lab</columnTitle>
        <description>Details regarding the provenance of the assay results.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#int</rangeURI>
      </column>
      <column columnName="specimen_type">
        <datatype>varchar</datatype>
        <columnTitle>Specimen type</columnTitle>
        <description>The type of specimen used in the assay. For NAb assays (Ad5 and HIV), this is generally serum or plasma.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="antigen_isolate_clade">
        <datatype>varchar</datatype>
        <columnTitle>Antigen Clade</columnTitle>
        <description>The clade (gene subtype) to which the antigen is most closely aligned. The clade may be a circulating type or a laboratory consensus or global clade.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="antigen_neutralization_tier">
        <datatype>varchar</datatype>
        <columnTitle>Envelope Neutralization Tier</columnTitle>
        <description>A classification specfic to HIV NAb assay design, in which an antigen is assessed for its ease of neutralization (1=most easily neutralized, 3=least easily neutralized)</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="antigen_isolate_name">
        <datatype>varchar</datatype>
        <columnTitle>Envelope Isolate Name</columnTitle>
        <description>The name of the ENV isolate used in the construction of the NAb antigen.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#string</rangeURI>
        <dimension>true</dimension>
      </column>
      <column columnName="nab_magnitude">
        <datatype>integer</datatype>
        <columnTitle>Magnitude</columnTitle>
        <description>Titer calculated at the specified inhibitory concentration.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#int</rangeURI>
      </column>
      <column columnName="response_call">
        <datatype>boolean</datatype>
        <columnTitle>Response call</columnTitle>
        <description>Indicates if neutralization is detected.</description>
        <rangeURI>http://www.w3.org/2001/XMLSchema#boolean</rangeURI>
        <dimension>true</dimension>
      </column>
    </columns>
    <tableTitle>Neutralizing antibody</tableTitle>
  </table>
