  <table tableName="Lab Results" tableDbType="TABLE">
    <description>Contains up to one row of Lab Results data for each Participant/visit combination.</description>
    <columns>
      <column columnName="ParticipantId">
        <datatype>varchar</datatype>
        <columnTitle>Participant ID</columnTitle>
        <description>Subject identifier</description>
